@model IEnumerable<Chat>
@using System.Security.Claims

@{
    var userId = User.FindFirst(ClaimTypes.NameIdentifier).Value;
}
<div class="chat-body">
        @foreach(var chat in Model)
        {
            <a class="room-button" asp-action ="GoToChat" asp-controller ="Home" asp-route-Id = "@chat.Id" >
                @chat.ChatUsers.FirstOrDefault(x =>x.UserId != userId ).User.UserName
            </a>
        }
</div>